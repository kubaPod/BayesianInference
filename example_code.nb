     RowBox[{"With", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"dsq", "=", 
         SuperscriptBox[
          RowBox[{"(", 
           FractionBox[
            RowBox[{"First", "[", 
             RowBox[{"Subtract", "[", 
              RowBox[{"#1", ",", "#2"}], "]"}], "]"}], "l"], ")"}], "2"]}], 
        "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        SuperscriptBox["s1", "2"], " ", "*", 
