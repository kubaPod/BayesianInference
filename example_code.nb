Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"mu", "=", "0"}], ",", 
     RowBox[{"logsigma", "=", 
      RowBox[{"-", "4"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Print", "@", 
     RowBox[{
      RowBox[{"obj", "[", "\"\<LogLikelihoodFunction\>\"", "]"}], "[", 
      RowBox[{"{", 
       RowBox[{"mu", ",", "logsigma"}], "}"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "@", 
     RowBox[{"LogLikelihood", "[", 
