   "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{"lim", ",", " ", 
     RowBox[{"{", 
      RowBox[{"\[Infinity]", ",", "10"}], "}"}]}], "}"}]}], 
  "\[IndentingNewLine]", "]"}]], "Input",
