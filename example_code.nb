    RowBox[{"\"\<Data\>\"", "\[Rule]", " ", "classificationData"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"\"\<GeneratingDistribution\>\"", " ", "\[Rule]", " ", 
     RowBox[{"BernoulliDistribution", "[", 
